# .env.example

# Rust core state directory (default: ./.meshclaw)
MESHCLAW_STATE_DIR=./node1-state

# WebSocket bridge port for Rust core (default: 3001)
MESHCLAW_BRIDGE_PORT=3001

# Gateway config path
OPENCLAW_CONFIG_PATH=./openclaw.json

# Gateway listen port
PORT=18795

# Rust bridge URL (gateway connects here)
RUST_BRIDGE_URL=ws://127.0.0.1:3001

# Log level for Rust core (error, warn, info, debug, trace)
RUST_LOG=info
